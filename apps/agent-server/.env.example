# Google AI API Key (get from https://aistudio.google.com/apikey)
GOOGLE_GENAI_API_KEY=your_api_key_here

# Server Configuration
PORT=3001

# Database (Supabase Postgres connection string)
# Local dev (after `supabase start`): postgres://postgres:postgres@127.0.0.1:54322/postgres
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# Supabase Configuration (optional; used only if you still call Supabase APIs)
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
